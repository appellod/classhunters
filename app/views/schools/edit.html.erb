<% provide(:title, "Edit School") %>
<% breadcrumbs({ 'Home'=>root_url, 'Schools'=>schools_path, @school.name=>school_path(@school), 'Edit'=>'' }) %>
<h1>Edit School</h1>

<%= form_for(@school) do |f| %>
  <%= render "form", f: f %>

  <%= f.submit "Save changes", class: "btn btn-large btn-primary" %>
<% end %>